u8 lcd_mod=0;
u8 sys_mod=1;
int p,r=1,k=1,n=0;
double v,mh,ml,v_old;
u8 b1,b11,b2,b22,b3,b33,b4,b44;
u8 mod2_index=0,mod2_r=1,mod2_k=1;
int tim_5s,tim_5s_con,b2_lock=1,tim_100;

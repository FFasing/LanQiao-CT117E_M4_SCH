/**
  * @brief  The application entry point.
  * @retval int
  */
int main(void)
{

  /* USER CODE BEGIN 1 */

  /* USER CODE END 1 */

  /* MCU Configuration--------------------------------------------------------*/

  /* Reset of all peripherals, Initializes the Flash interface and the Systick. */
  HAL_Init();

  /* USER CODE BEGIN Init */

  /* USER CODE END Init */

  /* Configure the system clock */
  SystemClock_Config();

  /* USER CODE BEGIN SysInit */

  /* USER CODE END SysInit */

  /* Initialize all configured peripherals */
  MX_GPIO_Init();
  MX_ADC2_Init();
  MX_TIM2_Init();
  MX_TIM3_Init();
  MX_TIM17_Init();
  /* USER CODE BEGIN 2 */
  /* USER CODE END 2 */

  /* Infinite loop */
  /* USER CODE BEGIN WHILE */
	LCD_Init();
	LCD_SetBackColor(Black);
	LCD_SetTextColor(White);
	LCD_Clear(Black);
	HAL_TIM_Base_Start_IT(&htim17);
	HAL_TIM_PWM_Start(&htim2,TIM_CHANNEL_2);
	TIM2->ARR=100-1;
	TIM2->PSC=200-1;
	HAL_TIM_IC_Start_IT(&htim3,TIM_CHANNEL_2);
  while (1)
  {
	  __HAL_TIM_SetCompare(&htim2,TIM_CHANNEL_2,p-1);
	  lcd();key();
    /* USER CODE END WHILE */

    /* USER CODE BEGIN 3 */
	  if(adc_lock)
	  {
		  HAL_ADC_Start(&hadc2);
		  v_r37=(HAL_ADC_GetValue(&hadc2)+1.0)/4000.0*3.3;
		  p=37.5*v_r37-27.5;
		  if(v_r37<=1) p=10;
		  if(v_r37>=3) p=85;
		  HAL_ADC_Stop(&hadc2);
	  }
	  HAL_GPIO_WritePin(GPIOC,(uint16_t)0xff00,1);
	  if(lcd_mod==0) HAL_GPIO_WritePin(GPIOC,GPIO_PIN_8,0);
	  if(tim_5s_con) HAL_GPIO_WritePin(GPIOC,GPIO_PIN_9,ld2);
	  if(adc_lock==0) HAL_GPIO_WritePin(GPIOC,GPIO_PIN_10,0);
	  HAL_GPIO_WritePin(GPIOD,GPIO_PIN_2,1);
	  HAL_GPIO_WritePin(GPIOD,GPIO_PIN_2,0);
  }
  /* USER CODE END 3 */
}


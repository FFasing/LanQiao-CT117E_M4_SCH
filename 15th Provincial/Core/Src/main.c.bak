void HAL_TIM_PeriodElapsedCallback(TIM_HandleTypeDef *htim)
{
	if(htim->Instance==TIM17)
	{
		if(tim_5s_con)
		{
			b2_lock=0;
			tim2_psc=TIM2->PSC;
			if(++tim100==100)
			{
				tim100=0;
				ld2^=1;
			}
			if(sys_mod==1&&TIM2->PSC<199) 
			{
				if(++tim_100==100)
				{
					TIM2->PSC+=(200-100)/(5000/100);
					tim_100=0;
				}
			}
			if(sys_mod==0&&TIM2->PSC>99) 
			{
				if(++tim_100==100)
				{
					TIM2->PSC-=(200-100)/(5000/100);
					tim_100=0;
				}
			}
			if(++tim_5s==5000)
			{
				b2_lock=1;
				tim_5s=0;
				tim_5s_con=0;
			}
		}
		if(tim_2s_con)
		{
			if(++tim_2s==2000)
			{
				tim_2s=0;
				tim_2s_con=0;
				adc_lock=0;
			}
		}
		if(++tim_2000==2000)
		{
			tim_2000=0;
			if(v_old==v)
			{
				if(sys_mod==1) ml=v_old;
				if(sys_mod==0) mh=v_old;
			}
			v_old=v;
		}

	}
}
void HAL_TIM_IC_CaptureCallback(TIM_HandleTypeDef *htim)
{
	if(htim->Instance==TIM3)
	{
		f=1000000.0/HAL_TIM_ReadCapturedValue(&htim3,TIM_CHANNEL_2);
		v=f*2.0*3.14*r/100.0/k;
	}
}
void lcd(void)
{
	u8 t[21];
	if(lcd_mod==0)
	{
		sprintf((char *)t,"        DATA        ");
		LCD_DisplayStringLine(Line1, (unsigned char *)t);
		if(sys_mod==0) sprintf((char *)t,"     M=H        ");
		if(sys_mod==1) sprintf((char *)t,"     M=L        ");
		LCD_DisplayStringLine(Line3, (unsigned char *)t);
		sprintf((char *)t,"     P=%d%%     ",p);
		LCD_DisplayStringLine(Line4, (unsigned char *)t);
		sprintf((char *)t,"     V=%.1f     ",v);
		LCD_DisplayStringLine(Line5, (unsigned char *)t);	
	}	
	if(lcd_mod==1)
	{
		sprintf((char *)t,"        PARA        ");
		LCD_DisplayStringLine(Line1, (unsigned char *)t);
		sprintf((char *)t,"     R=%d        ",mod2_r);
		LCD_DisplayStringLine(Line3, (unsigned char *)t);
		sprintf((char *)t,"     K=%d        ",mod2_k);
		LCD_DisplayStringLine(Line4, (unsigned char *)t);
	}
	if(lcd_mod==2)
	{
		sprintf((char *)t,"        RECD        ");
		LCD_DisplayStringLine(Line1, (unsigned char *)t);
		sprintf((char *)t,"     N=%d        ",n);
		LCD_DisplayStringLine(Line3, (unsigned char *)t);
		sprintf((char *)t,"     MH=%.1f     ",mh);
		LCD_DisplayStringLine(Line4, (unsigned char *)t);
		sprintf((char *)t,"     ML=%.1f     ",ml);
		LCD_DisplayStringLine(Line5, (unsigned char *)t);
	}
}
void key(void)
{
	if(HAL_GPIO_ReadPin(GPIOB,GPIO_PIN_0)==0) b1=1;
	else b1=0;
	if(b1==1&&b11==0)
	{
		b11=1;
		LCD_Clear(Black);
		if(++lcd_mod==3) lcd_mod=0;
		if(lcd_mod==1) mod2_index=0;
		if(lcd_mod==2) 
		{
			r=mod2_r;
			k=mod2_k;
		}
	}if(b1==0&&b11==1) b11=0;
	
	if(HAL_GPIO_ReadPin(GPIOB,GPIO_PIN_1)==0) b2=1;
	else b2=0;
	if(b2==1&&b22==0)
	{
		b22=1;
		if(lcd_mod==0&&b2_lock) 
		{
			n++;
			tim_5s_con=1;
			sys_mod^=1;
		}
		if(lcd_mod==1) mod2_index^=1;
	}if(b2==0&&b22==1) b22=0;
	
	if(HAL_GPIO_ReadPin(GPIOB,GPIO_PIN_2)==0) b3=1;
	else b3=0;
	if(b3==1&&b33==0)
	{
		b33=1;
		if(lcd_mod==1) 
		{
			if(mod2_index==0)
			{
				mod2_r++;
				if(mod2_r==11) mod2_r=1;
			}
			if(mod2_index==1)
			{
				mod2_k++;
				if(mod2_k==11) mod2_k=1;
			}
		}
	}if(b3==0&&b33==1) b33=0;
	
	if(HAL_GPIO_ReadPin(GPIOA,GPIO_PIN_0)==0) b4=1;
	else b4=0;
	if(b4==1&&b44==0)
	{
		b44=1;
		if(lcd_mod==1) 
		{
			if(mod2_index==0)
			{
				mod2_r--;
				if(mod2_r==0) mod2_r=10;
			}
			if(mod2_index==1)
			{
				mod2_k--;
				if(mod2_k==0) mod2_k=10;
			}
		}
		if(lcd_mod==0)
		{
			if(adc_lock)
			{
				tim_2s_con=1;
				tim_2s=0;
			}
			else
			{
				adc_lock=1;
			}
		}
	}if(b4==0&&b44==1) 
	{
		b44=0;
		tim_2s_con=0;
		tim_2s=0;
		
	}
}
